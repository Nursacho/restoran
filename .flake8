[flake8]
max-line-length = 120
max-complexity = 18
select = B,C,E,F,G,W,T4,B9
#ignore = E203, E266, E501, W503
ignore =
    E126,
    E401,
    E402,
    E701,
    E731,
    W291,
    F401  # Ignore not used import
    W504,
    E124,


exclude =
    .venv,
    .git,
    .tox,
    venv,
    dist,
    doc,
    *openstack/common/*,
    *lib/python*,
    *egg,
    build,
    tools/xenserver*,
    releasenotes,
    */migrations/*
